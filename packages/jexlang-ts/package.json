{
  "name": "jexlang-ts",
  "version": "1.0.0",
  "description": "A lightweight expression language for JavaScript and TypeScript",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "expression",
    "language",
    "evaluator",
    "parser",
    "formula",
    "rule-engine",
    "javascript",
    "typescript"
  ],
  "author": "vinaykumar0339 <vinay9961@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vinaykumar0339/jexlang.git"
  },
  "homepage": "https://github.com/vinaykumar0339/jexlang",
  "bugs": {
    "url": "https://github.com/vinaykumar0339/jexlang/issues"
  },
  "devDependencies": {
    "tsup": "8.5.0",
    "typescript": "5.8.3",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "antlr4": "4.13.2"
  }
}
